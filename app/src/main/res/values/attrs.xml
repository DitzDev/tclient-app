<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string" />
    <attr name="subtitle" format="string" />
    <attr name="icon" format="reference" />
    <attr name="checked" format="boolean" />
    
    <declare-styleable name="SettingsSwitch">
        <attr name="title" />
        <attr name="subtitle" />
        <attr name="icon" />
        <attr name="checked" />
        <attr name="android:enabled" />
    </declare-styleable>
    
    <declare-styleable name="SettingsRadioCheck">
        <attr name="title" />
        <attr name="subtitle" />
        <attr name="icon" />
        <attr name="checked" />
    </declare-styleable>    
    
    <declare-styleable name="SettingsLayout">
        <attr name="title" />
        <attr name="subtitle" />
        <attr name="icon" />
    </declare-styleable>
</resources>